{
  "name": "rizztorante-nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d database/data-source.ts",
    "migration:create": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create -d database/data-source.ts",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d database/data-source.ts",
    "migration:show": "pnpm run typeorm -- migration:show -d database/data-source.ts",
    "migration:revert": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d database/data-source.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky",
    "seed": "ts-node -r tsconfig-paths/register src/seed.ts",
    "test:load": "k6 run test/performance/app-user-flow-performance-test.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.645.0",
    "@aws-sdk/s3-request-presigner": "3.645.0",
    "@fastify/secure-session": "8.1.1",
    "@fastify/static": "8.1.1",
    "@fastify/swagger-ui": "5.2.2",
    "@nestjs-modules/ioredis": "2.0.2",
    "@nestjs/common": "^11.0.11",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^11.0.11",
    "@nestjs/event-emitter": "2.0.4",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "^11.0.11",
    "@nestjs/platform-fastify": "11.0.11",
    "@nestjs/schedule": "4.1.0",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "6.2.1",
    "@nestjs/typeorm": "^10.0.2",
    "aws-sdk": "2.1691.0",
    "bcrypt": "5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "1.7.4",
    "connect-redis": "6.1.3",
    "connect-typeorm": "2.0.0",
    "express-session": "1.18.0",
    "fastify-static": "4.7.0",
    "firebase-admin": "12.5.0",
    "ioredis": "5.4.1",
    "nestjs-pino": "4.1.0",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "pg": "^8.14.0",
    "pino": "9.4.0",
    "pino-pretty": "11.2.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sharp": "0.33.5",
    "slugify": "^1.6.6",
    "stripe": "16.9.0",
    "typeorm": "^0.3.21",
    "typeorm-naming-strategies": "4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.4.1",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/types": "^19.8.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^11.0.11",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.3.5",
    "@types/bcrypt": "5.0.2",
    "@types/compression": "1.7.5",
    "@types/connect-redis": "0.0.23",
    "@types/express": "^4.17.21",
    "@types/express-session": "1.18.0",
    "@types/ioredis": "5.0.0",
    "@types/jest": "^29.5.14",
    "@types/k6": "0.54.1",
    "@types/multer": "1.4.12",
    "@types/node": "^20.17.24",
    "@types/passport": "1.0.16",
    "@types/passport-jwt": "4.0.1",
    "@types/passport-local": "1.0.38",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "commitlint": "^19.8.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "9.1.5",
    "jest": "^29.7.0",
    "k6": "0.0.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm-extension": "3.7.0",
    "typescript": "^5.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@common/(.*)$": "<rootDir>/_common/$1",
      "^@events/(.*)$": "<rootDir>/_events/$1",
      "^@/(.*)$": "<rootDir>/$1"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}
